{% extends '::base.html.twig' %}

{#% block customcss %}
    {% stylesheets '@CBAccountBundle/Resources/css/LabelCollection.css' %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}

{% block customjs %}
    {% javascripts '@CBAccountBundle/Resources/js/AccountForm/*' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %#}

{% block title %} Register {% endblock %}

{% block body %}

    {{ form_start(form, { 'style': 'horizontal' }) }}
        <div class="row error-log" style="margin: 20px auto;">
            <div class="col-lg-offset-2">
                {{ form_errors(form) }}
            </div>
        </div>
           
        <div class="row">    
            {{ form_row(form.firstName) }}
            {{ form_row(form.middleName) }}
            {{ form_row(form.lastName) }}
            {{ form_row(form.birthdate) }}
            {{ form_row(form.password) }}
            {{ form_row(form.contactEmail) }}
